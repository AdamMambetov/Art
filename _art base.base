formulas:
  Untitled: ""
views:
  - type: cards
    name: Cards
    filters:
      and:
        - file.basename != "_art base"
        - file.basename != "_database"
        - '!file.inFolder("covers")'
        - '!file.inFolder("templates")'
    order:
      - Name
      - Rating
      - Status
      - Episode
      - Year
      - Views
      - Type
    sort:
      - property: Name
        direction: ASC
    image: note.Cover
