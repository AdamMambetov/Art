formulas:
  Untitled: ""
  name: aliases[0]
views:
  - type: cards
    name: Cards
    filters:
      and:
        - file.basename != "_art base"
        - file.basename != "_database"
        - '!file.inFolder("covers")'
        - '!file.inFolder("templates")'
    order:
      - formula.name
      - Rating
      - Status
      - Episode
      - Year
      - Views
    sort:
      - property: Type
        direction: ASC
      - property: formula.name
        direction: ASC
    image: note.Cover
    cardSize: 130
